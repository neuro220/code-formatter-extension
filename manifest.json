 {
  "name": "__MSG_extName__",
  "version": "1.7.0",
  "manifest_version": 3,
  "description": "__MSG_extDescription__",
  "icons": {
    "128": "icons/128.png",
    "48": "icons/48.png",
    "32": "icons/32.png",
    "16": "icons/16.png"
  },
  "background": {
    "scripts": ["background.min.js"]
  },
  "permissions": [
    "scripting",
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*",
    "file:///*"
  ],
  "options_ui": {
    "page": "options.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "FirefoxCodeFormatter@example.org",
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  },
  "default_locale": "en"
}
